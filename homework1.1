import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        int a = N/10;
        var f = N/100+a%10 + N%10;
        var b = ((N-1)%2)*2+N%2+N;
        System.out.println(f);
        System.out.println(b);
    }
}
